{
    "compilerOptions": {
        "baseUrl": ".",
        "verbatimModuleSyntax": false,
        "paths": {
            "@/*": ["src/*"],
            "@/components": ["src/components/*"],
            "@/icons": ["src/icons/*"],
            "@/lib": ["src/lib/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    },
    "extends": "astro/tsconfigs/strict"
}
